   SubscriptBox["G", "R"], 
   RowBox[{"(", 
    RowBox[{"x", "-", "y"}], ")"}]}], " ", "=", " ", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"D", 
      RowBox[{"(", 
       RowBox[{"x", "-", "y"}], ")"}]}], " ", "-", " ", 
